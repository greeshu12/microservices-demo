@Library('jenkins-shared-library') _
pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo "Running build for Service A"
                myBuild()
            }
        }

        stage('Deploy') {
            steps {
                myDeploy("service1")
            }
        }
    }

    post {
        always {
            myNotify("Service A pipeline completed")
        }
    }
}
